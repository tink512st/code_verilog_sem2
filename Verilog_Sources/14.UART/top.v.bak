module top#(
	parameter clk_freq = 1000000,
	parameter baud_rate = 9600
)(donerx,donetx,doutrx,tx,clk,rst,rx,newd,dintx);
output donerx,donetx,tx;
output [7:0] doutrx;
input clk,rst,rx,newd;
input [7:0] dintx;

uarttx md_tx#( 					
	.clk_freq(clk_freq),
	.baud_rate(baud_rate)
)
(.doneTx(donetx),.tx(tx),.clk(clk),.rst(rst),.new_data(newd),.tx_data(dintx));
uartrx#(
	.clk_freq(clk_freq),
	.baud_rate(baud_rate)
)
(.doneRx(donerx),.rxData(doutrx),.clk(clk),.rst(rst),.rx(rx));
endmodule